@model IEnumerable<RPRO_SportSoft.Models.Sportoviste>

@{
    ViewBag.Title = "Sportoviste";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Přidaní sportoviště</h2>
@Html.ActionLink("Main", "Index", "Main")
<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table" border="1">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Jmeno)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model)
{
    
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Jmeno)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.ID }) |
                @Html.ActionLink("Details", "Details", new { id = item.ID }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.ID })
            </td>
        </tr>
   
}

    </table>
