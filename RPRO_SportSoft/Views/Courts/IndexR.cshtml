@model IEnumerable<RPRO_SportSoft.Reservation>

@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Výpis rezervací</h2>

<table class="table">
    <tr>
        <th>
            Datum a čas
        </th>
        <th>
            Kurt
        </th>
        <th>
            Cena
        </th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Date)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Courts_Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Price)
            </td>
        </tr>
    }

</table>

@Html.ActionLink("Zpět", "Index", "Sports")
<br />